#VRML V1.0 ascii

Separator {
    Coordinate3 {
        point [
            -1 -1 0,
             1 -1 0,
            -1  1 0,
             1  1 0,
        ]
    }

    TextureCoordinate2 {
        point [
            0 0,
            1 0,
            0 1,
            1 1,
        ]
    }

    IndexedFaceSet {
        coordIndex [
            0, 2, 3, 1, -1,
            0, 1, 3, 2, -1,
        ]
        textureCoordIndex [
            0, 2, 3, 1, -1,
            0, 1, 3, 2, -1,
        ]
    }
}
